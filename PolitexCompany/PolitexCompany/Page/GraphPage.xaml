<Page x:Class="PolitexCompany.Page.GraphPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:ScottPlot="clr-namespace:ScottPlot;assembly=ScottPlot.WPF"
      xmlns:local="clr-namespace:PolitexCompany.Page"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GraphPage">

    <Grid>
        <WpfPlot x:Name="Graph" Margin="94,68,94,68"></WpfPlot>
        <ComboBox x:Name="YearComboBox" SelectionChanged="YearComboBox_SelectionChanged" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="200">
            <ComboBoxItem Content="2023"/>
            <ComboBoxItem Content="2024"/>
        </ComboBox>
        <Label Content="ГРАФИК" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="109" FontSize="24" FontWeight="Bold"/>

    </Grid>
</Page>
